<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
    <package>org.lazydog.jdnsaas.model</package>
    <entity class="DNSServer" access="FIELD">
        <table name="dns_server" schema="jdnsaas"/>
        <attributes>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
            <basic name="port">
                <column name="port" nullable="false"/>
            </basic>
            <many-to-one name="transactionSignature" fetch="EAGER">
                <join-column name="transaction_signature_id" nullable="false"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
