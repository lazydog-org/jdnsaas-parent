<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
    <package>org.lazydog.jdnsaas.model</package>
    <entity class="Resolver" access="FIELD">
        <table name="resolver" schema="jdnsaas"/>
        <attributes>
            <basic name="hostName">
                <column name="host_name" nullable="false"/>
            </basic>
            <basic name="localHostName">
                <column name="local_host_name" nullable="true"/>
            </basic>
            <basic name="port">
                <column name="port" nullable="false"/>
            </basic>
            <many-to-one name="tsigKey" fetch="EAGER">
                <join-column name="tsig_key_id" nullable="true"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
